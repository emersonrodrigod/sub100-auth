{
    "name": "sub100/auth",
    "description": "Sub100 Auth Laravel SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "Emerson Dario",
            "email": "emersonrodrigod@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
          "sub100\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sub100\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "sub100\\Auth\\Sub100AuthServiceProvider"
            ]
        }
    }
}
